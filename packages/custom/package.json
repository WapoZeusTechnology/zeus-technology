{
  "name": "@zeus-technology/custom",
  "version": "1.1.1",
  "description": "A simple library to facilitate custom enhancements to Zeus",
  "keywords": [
    "zeus",
    "ssp",
    "zeus-technology"
  ],
  "author": "Mike Stemle <mike.stemle@washpost.com>",
  "homepage": "http://www.zeustechnology.com",
  "license": "MIT",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WapoZeusTechnology/zeus-technology.git"
  },
  "scripts": {
    "build": "BABEL_ENV=build babel . --out-dir dist --source-maps --delete-dir-on-start --config-file ./babel.config.js --no-comments --copy-files --ignore 'static/*'; browserify ./static/dist-builder.js --standalone ZeusCustom -o ./dist/zeus-adapter.static.js",
    "clean": "rm -rf dist/*",
    "test": "jest"
  },
  "bugs": {
    "url": "https://github.com/WapoZeusTechnology/zeus-technology/issues"
  },
  "dependencies": {
    "eventemitter3": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@types/eventemitter3": "^2.0.2",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.11.2",
    "browserify": "^16.5.0"
  }
}